{
    "name": "javck/ecpay",
    "description": "EcPay Library for Laravel Voyager",
    "type": "liberary",
    "keywords": ["payment", "ecpay", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "zack lin",
            "email": "javck.zack@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "~5.7.0|~5.8.0",
        "fideloper/proxy": "^4.0",
        "laravelcollective/html": "^5.4.0"
    },
    "require-dev":{
        "laravel/tinker": "^1.0"
    },
    "autoload": {  
        "psr-4": {
            "Javck\\Ecpay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Javck\\Ecpay\\EcPayServiceProvider"
            ],
            "aliases": {
                "Ecpay": "Javck\\Ecpay\\Facades\\EcPay"
            }
        }
    }
}